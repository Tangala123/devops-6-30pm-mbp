pipeline{
  agent any
  stages{
   stage("main"){
    when{
      branch "main"
    }
     steps{ 
       echo "scm .........."
     }
   }
   stage("main1"){
    when{
      branch "main"
    }
     steps{ 
       echo "build .........."
     }
   }
   stage("develop"){
    when{
      branch "develop"
    }
     steps{ 
       echo "deploy .........."
     }
   }
  }
}
